# Environment variables and secrets - NOT tracked in git
# Copy this file to ~/.zsh.env and fill in your actual tokens
# This file is sourced by ~/.zshrc if it exists

# GitHub tokens
export GITHUB_TOKEN=your_github_token_here
export GPR_AUTH_TOKEN=your_gpr_token_here

# AWS tokens
export AWS_BEARER_TOKEN_BEDROCK=your_aws_token_here

